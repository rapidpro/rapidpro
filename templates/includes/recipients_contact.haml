-load contacts i18n

-if contact.is_active
  .lbl-group.linked.mr-2.mb-2(onclick="goto(event)" href="{% url 'contacts.contact_read' contact.uuid %}")
    %temba-icon(name="icon.contact")
    {{ contact|name_or_urn:user_org }}
-else
  .lbl-group.mr-2.mb-2
    %temba-icon(name="icon.contact")
    .line-through
      -trans "Deleted"